{
  "name": "long-run-hati",
  "version": "1.0.0",
  "description": "",
  "author": "Brad Parker <hi@bradparker.com>",
  "license": "ISC",
  "main": "dist/index.html",
  "private": true,
  "engines": {
    "node": "5.2.0",
    "npm": "3.3.12"
  },
  "scripts": {
    "build:setup": "rm -rf .dev/* dist/* && mkdir -p dist .dev",
    "build:prod": "npm run build:setup && ENV_FILE=.env.production gulp build",

    "build:staging": "npm run build:setup && ENV_FILE=.env.staging gulp build",

    "build:dev": "npm run build:setup && ENV_FILE=.env.development gulp build",

    "dev": "ENV_FILE=.env.development gulp dev",

    "test": "mocha --compilers js:./build/babel-register,css:css-modules-require-hook source/**/__tests__/*-test.js",

    "deploy:staging": "npm run build:staging && gh-pages -d dist",

    "deploy:prod": "npm run build:prod && S3_TARGETS='http://thelongrunforhaiti.everydayhero.do/' gulp deploy:prod"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ]
  },
  "standard": {
    "globals": [
      "describe",
      "context",
      "it",
      "before",
      "after",
      "$"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "ecmaFeatures": {
      "arrowFunctions": true,
      "blockBindings": true,
      "classes": true,
      "destructuring": true,
      "experimentalObjectRestSpread": true,
      "jsx": true,
      "modules": true,
      "objectLiteralComputedProperties": true,
      "objectLiteralShorthandMethods": true,
      "objectLiteralShorthandProperties": true,
      "templateStrings": true
    }
  },
